{
  "name": "civil-services-city-council-az-phoenix",
  "version": "1.0.0",
  "description": "City Council Data for Sacramento, CA ( JSON, XML, CSV, SQL & YML formats )",
  "license": "MIT",
  "keywords": [
    "Civil Services",
    "US",
    "United States",
    "City Council",
    "Phoenix",
    "AZ",
    "data",
    "json",
    "csv",
    "yml",
    "sql",
    "xml",
    "xlsx",
    "headshots",
    "pictures",
    "photos",
    "map"
  ],
  "cityData": {
    "state_code": "AZ",
    "city_name": "Phoenix",
    "city_population": "1563025",
    "city_website": "https://www.phoenix.gov",
    "public_records": "https://www.phoenix.gov/cityclerk/public-records",
    "latitude": "33.5722",
    "longitude": "-112.088"
  },
  "main": "data/city-council.json",
  "homepage": "https://civilserviceusa.github.io/city-council-az-phoenix/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/civilserviceusa/city-council-az-phoenix.git"
  },
  "bugs": {
    "url": "https://github.com/civilserviceusa/city-council-az-phoenix/issues"
  },
  "author": {
    "name": "Peter Schmalfeldt",
    "email": "me@peterschmalfeldt.com",
    "url": "https://peterschmalfeldt.com"
  },
  "scripts": {
    "json-to-yml": "./node_modules/.bin/json2yaml ./city-council/data/city-council-data.json > ./city-council/data/city-council-data.yml && echo '✓ Updated city-council/data/city-council-data.yml'",
    "json-to-xml": "node ./.scripts/json-to-xml.js && echo '✓ Updated city-council/data/city-council-data.xml'",
    "csv-to-sql": "node ./.scripts/csv-to-sql.js && echo '✓ Updated city-council/data/city-council-data.sql'",
    "csv-to-json": "./node_modules/.bin/csvtojson ./city-council/data/city-council-data.csv > ./city-council/data/city-council-data.json && echo '✓ Updated city-council/data/city-council-data.json'",
    "resize-headshots": "node ./.scripts/resize-headshots.js",
    "resize-backgrounds": "node ./.scripts/resize-backgrounds.js",
    "convert-csv": "node ./.scripts/convert-csv.js",
    "build-images": "npm run -s resize-headshots && npm run -s resize-backgrounds && echo '\n☆ Image Conversion Complete\n'",
    "build-data": "npm run -s csv-to-json && npm run -s json-to-yml && npm run -s json-to-xml && npm run -s csv-to-sql && echo '\n☆ Data Conversion Complete\n'",
    "build": "npm run -s convert-csv && npm run -s build-data && npm run -s build-images && echo '\n☆ヽ(´▽｀)ノ All Done \n'"
  },
  "devDependencies": {
    "csvtojson": "^1.1.2",
    "fast-csv": "^2.3.0",
    "glob": "^7.1.1",
    "gm": "^1.23.0",
    "js2xmlparser": "^2.0.2",
    "json2csv": "^3.7.3",
    "json2yaml": "^1.1.0",
    "object.values": "^1.0.4",
    "slug": "^0.9.1",
    "stream-replace": "^1.0.0"
  }
}
